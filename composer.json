{
  "name": "avto-dev/moneta-api-php",
  "description": "PHP package for a working with Moneta.ru service",
  "keywords": [
    "php",
    "moneta",
    "client",
    "api"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Avto Develop",
      "homepage": "https://github.com/avto-dev"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.3",
    "nesbot/carbon": "^1.22",
    "psr/http-message": "1.*"
  },
  "require-dev": {
    "avto-dev/php-cs-fixer": "^1.0",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "~5.7 || ^6.4",
    "symfony/var-dumper": "^3.3.10"
  },
  "autoload": {
    "psr-4": {
      "AvtoDev\\MonetaApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AvtoDev\\MonetaApi\\Tests\\": "tests"
    }
  },
  "support": {
    "issues": "https://github.com/avto-dev/moneta-api/issues",
    "source": "https://github.com/avto-dev/moneta-api"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --no-coverage",
    "cs": "./vendor/bin/cs-fix"
  },
  "config": {
    "sort-packages": true
  }
}
